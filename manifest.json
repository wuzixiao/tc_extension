{
  "manifest_version": 2,

  "name": "Transcode",
  "description": "This extension will try to give you a better feeling when you are reading articles",
  "version": "0.1",

  "background": {"page": "bg.html"},
  "page_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["tabs"],
  "content_scripts": [ 
	{
		"matches": ["*://*/*"],
		"js": ["content.js"]
	}
  ]
}
